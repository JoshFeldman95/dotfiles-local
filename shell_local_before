path_prepend "$HOME/bin"

# Homebrew and Related Tools
path_prepend '/usr/local/sbin'
path_prepend '/usr/local/bin'
path_prepend '/usr/local/opt/coreutils/libexec/gnubin'
path_prepend '/usr/local/opt/make/libexec/gnubin'
path_prepend '/usr/local/opt/python/libexec/bin'

# CrossPack
path_append "/usr/local/CrossPack-AVR/bin"

# RISC-V toolchain
path_append "/usr/local/opt/riscv-gnu-toolchain/bin"

source ~/.dotfiles_local/bootstrap.sh
